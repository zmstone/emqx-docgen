connectors.confluent_producer.my_webhook_1.ssl {
  cacertfile = "/path/to/ca-certificates-bundle.pem"
  certfile = "/path/to/certificate-chain.pem"
  keyfile = "/path/to/private-key.pem"
  reuse_sessions = true
  depth = 10
  password = "your-password"
  versions = ["tlsv1.3", "tlsv1.2"]
  ciphers = []
  secure_renegotiate = true
  log_level = notice
  hibernate_after = "5s"
  partial_chain = false
  verify_peer_ext_key_usage = "clientAuth,serverAuth"
  server_name_indication = auto
}