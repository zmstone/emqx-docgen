dashboard.sso.ldap {
  enable = false
  backend = ldap
  query_timeout = "5s"
  server = "example.ldap.server"
  pool_size = 8
  username = "example_user"
  password = "example_password"
  base_dn = "dc=example,dc=com"
  filter = "(& (objectClass=person) (uid=${username}))"
  request_timeout = "10s"
  ssl = {
    #substruct(ldap:ssl)
  }
}