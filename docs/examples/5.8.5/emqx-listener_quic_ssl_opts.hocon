listeners.quic.my_quic_listener.ssl_options {
  cacertfile = "${EMQX_ETC_DIR}/certs/cacert.pem"
  certfile = "${EMQX_ETC_DIR}/certs/cert.pem"
  keyfile = "${EMQX_ETC_DIR}/certs/key.pem"
  verify = verify_none
  hibernate_after = "5s"
  # password is intentionally left out as it is optional and might be sensitive.
}