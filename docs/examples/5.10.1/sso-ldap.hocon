dashboard.sso.ldap {
  enable = false
  backend = ldap
  query_timeout = "5s"
  server = "host:389"
  pool_size = 8
  username = "example_user"
  password = "example_password"
  request_timeout = "10s"
  ssl = {
    #substruct(ldap:ssl)
  }
  base_dn = "dc=example,dc=com"
  filter = "(& (objectClass=person) (uid=${username}))"
}