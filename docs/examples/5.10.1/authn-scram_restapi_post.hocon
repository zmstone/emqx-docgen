authentication = [
  {
    method = post
    headers = {accept = "application/json", cache-control = "no-cache", connection = "keep-alive", content-type = "application/json", keep-alive = "timeout=30, max=1000"}
    precondition = ""
    mechanism = scram
    backend = http
    algorithm = sha256
    iteration_count = 4096
    url = "http://example.com/auth"
    body = {key = "value"}  # Example placeholder for actual request body content
    request_timeout = "5s"
    connect_timeout = "15s"
    pool_size = 8
    enable_pipelining = 100
    max_inactive = "10s"
    request = {
      #substruct(connector_http:request)
    }
    ssl = {enable = false}
  }
]